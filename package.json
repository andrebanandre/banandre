    "query-string": "^9.3.1",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-syntax-highlighter": "^16.1.0",
    "schema-dts": "^1.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@opennextjs/cloudflare": "^1.14.4",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
